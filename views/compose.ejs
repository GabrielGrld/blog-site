<%- include("partials/header") %>

<h1>Compose</h1>
<form id="post_form" class="" action="/compose" method="post">

  <div class="form-group">
    <label>Title</label>
    <input type="text" class="form-control" name="postTitle" autocomplete="off">
  </div>

  <label>Post</label>
  <div class="form-group">
    <textarea name="postBody" class="form-control" rows="5"></textarea>
  </div>

  <button type="submit" name="button" class="btn btn-primary"> Publish</button>

</form>

<%- include("partials/footer") %>
